{
  "name": "psx/framework",
  "description": "Engine of the PSX framework",
  "keywords": ["psx", "rest", "api", "framework"],
  "homepage": "http://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com/"
    }
  ],
  "require": {
    "php": ">=5.5",
    "doctrine/annotations": "^1.2",
    "doctrine/dbal": "^2.5",
    "jms/serializer": "^1.1",
    "monolog/monolog": "^1.17",
    "psr/log": "^1.0",
    "psx/api": "^1.0",
    "psx/cache": "^1.0",
    "psx/data": "^1.0",
    "psx/http": "^1.0",
    "psx/json": "^1.0",
    "psx/oauth": "^1.0",
    "psx/oauth2": "^1.0",
    "psx/schema": "^1.0",
    "psx/sql": "^1.0",
    "psx/uri": "^1.0",
    "psx/validate": "^1.0",
    "symfony/console": "~2.8|~3.0",
    "symfony/dependency-injection": "~3.0",
    "symfony/event-dispatcher": "~2.8|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "phpunit/dbunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "PSX\\Framework\\": "src/"
