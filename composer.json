{
  "name": "psx/framework",
  "description": "Engine of the PSX framework",
  "keywords": ["psx", "rest", "api", "framework"],
  "homepage": "https://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "https://chrisk.app"
    }
  ],
  "require": {
    "php": ">=8.0",
    "monolog/monolog": "^2.3",
    "psr/log": "^1.0|^2.0|^3.0",
    "psr/container": "^2.0",
    "psx/api": "^4.0",
    "psx/data": "^4.0",
    "psx/engine": "^1.0",
    "psx/dependency": "^2.0",
    "psx/http": "^3.0",
    "psx/json": "^2.0",
    "psx/oauth2": "^3.0",
    "psx/schema": "^4.0",
    "psx/sql": "^3.0",
    "psx/uri": "^2.0",
    "psx/validate": "^2.0",
    "symfony/cache": "^5.4",
    "symfony/console": "^4.0|^5.0",
    "symfony/event-dispatcher": "^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "guzzlehttp/guzzle": "^6.3",
    "vimeo/psalm": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "PSX\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
