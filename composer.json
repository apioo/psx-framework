{
  "name": "psx/framework",
  "description": "Engine of the PSX framework",
  "keywords": [
    "psx",
    "rest",
    "api",
    "framework"
  ],
  "homepage": "https://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "https://chrisk.app"
    }
  ],
  "require": {
    "php": ">=8.1",
    "psr/cache": "^1.0|^2.0|^3.0",
    "psr/log": "^1.0|^2.0|^3.0",
    "psr/container": "^1.0|^2.0",
    "doctrine/migrations": "^3.6",
    "monolog/monolog": "^2.3|^3.0",
    "psx/api": "^6.0",
    "psx/data": "^6.0",
    "psx/engine": "^2.0",
    "psx/http": "^4.0",
    "psx/json": "^3.0",
    "psx/oauth2": "^4.0",
    "psx/schema": "^6.0",
    "psx/sql": "^4.0",
    "psx/uri": "^3.0",
    "psx/validate": "^2.0",
    "symfony/cache": "^5.0|^6.0",
    "symfony/console": "^5.0|^6.0",
    "symfony/event-dispatcher": "^5.0|^6.0",
    "symfony/dependency-injection": "^5.0|^6.0",
    "symfony/config": "^5.0|^6.0",
    "symfony/dotenv": "^5.0|^6.0",
    "symfony/mailer": "^5.0|^6.0",
    "symfony/messenger": "^5.0|^6.0",
    "symfony/doctrine-messenger": "^5.0|^6.0",
    "symfony/serializer": "^5.0|^6.0",
    "symfony/rate-limiter": "^5.0|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "guzzlehttp/guzzle": "^7.0",
    "vimeo/psalm": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "PSX\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PSX\\Framework\\Tests\\": "tests/",
      "PSX\\Framework\\App\\": "app/"
    }
  }
}
