{
  "name": "psx/framework",
  "description": "Engine of the PSX framework",
  "keywords": ["psx", "rest", "api", "framework"],
  "homepage": "http://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com/"
    }
  ],
  "require": {
    "php": ">=7.2",
    "doctrine/annotations": "^1.5",
    "doctrine/dbal": "~2.9.0",
    "monolog/monolog": "^1.17",
    "psr/log": "^1.0",
    "psr/container": "^1.0",
    "psx/api": "^3.0",
    "psx/cache": "^1.0",
    "psx/data": "^3.0",
    "psx/engine": "^0.1",
    "psx/dependency": "^1.0",
    "psx/http": "^2.0",
    "psx/json": "^1.0",
    "psx/oauth": "^2.0",
    "psx/oauth2": "^2.0",
    "psx/schema": "^3.0",
    "psx/sql": "^2.0",
    "psx/uri": "^1.0",
    "psx/validate": "^1.0",
    "symfony/console": "^4.0|^5.0",
    "symfony/event-dispatcher": "^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "guzzlehttp/guzzle": "^6.3",
    "vimeo/psalm": "^3.11"
  },
  "autoload": {
    "psr-4": {
      "PSX\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PSX\\Framework\\Tests\\": "tests/",
      "PSX\\Framework\\App\\": "app/"
    }
  }
}
